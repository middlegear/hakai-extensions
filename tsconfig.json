{
  "include": ["src"],
  "exclude": ["**/*.test.ts","node_modules"],
  "compilerOptions": {
    "target": "ESNext" ,
    "module": "ESNext" ,
    "moduleResolution": "node", 
    "noEmit": true,
    "emitDeclarationOnly": false,
    "isolatedModules":false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    // "noUncheckedIndexedAccess": true, 
    "declaration": true,
    "skipLibCheck": true ,
    "lib": ["ESNext"],
    
  }
}