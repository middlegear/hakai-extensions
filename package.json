{
  "name": "hakai-extensions",
  "version": "1.0.2",
  "description": "scraper package serving anime media infomation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "test-jikan": "vitest jikan.test.ts   --config vitest.config.ts",
    "test-anilist": "vitest anilist.test.ts  --config vitest.config.ts",
    "test-hianime": "vitest hianime.test.ts --silent run --config vitest.config.ts",
    "test-animekai": "vitest animekai.test --config vitest.config.ts",
    "test": "vitest --silent run --config vitest.config.ts",
    "lint": "tsc",
    "ci": "tsc && pnpm test && pnpm run build",
    "format": "prettier --write ."
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "scraper"
  ],
  "author": "bravinjoshua",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bravinjoshua/hakai.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "prettier": "^3.5.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "crypto-js": "^4.2.0"
  }
}
