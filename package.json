{
  "name": "hakai-extensions",
  "version": "1.0.8",
  "description": "scraper package serving anime media infomation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "test-jikan": "vitest jikan.test.ts   --config vitest.config.ts",
    "test-anilist": "vitest anilist.test.ts  --config vitest.config.ts",
    "test-hianime": "vitest hianime.test.ts --silent run --config vitest.config.ts",
    "test-animekai": "vitest animekai.test --config vitest.config.ts",
    "test": "vitest --silent run --config vitest.config.ts",
    "lint": "tsc",
    "ci": "tsc && pnpm test && pnpm run build",
    "format": "prettier --write ."
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "scrapper",
    "anime"
  ],
  "author": "bravinjoshua",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/middlegear/hakai-extensions.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/middlegear/hakai-extensions#readme",
  "devDependencies": {
    "@types/node": "^22.13.10",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.8.3",
    "cheerio": "^1.0.0",
    "crypto-js": "^4.2.0",
    "got-scraping": "^4.1.1"
  },
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6"
}
